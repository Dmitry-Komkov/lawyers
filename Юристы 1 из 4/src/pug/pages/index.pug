extends ../layout/main

block variables
  - var title = 'Webpack + Pug';
  - var description = 'Description for Webpack + Pug';
  - var pageClass = 'index-page';

block content
  .modal.fade#modal-form(tabindex="-1" role="dialog" aria-labelledby="modalFormLabel" aria-hidden="true")
    .modal-dialog.modal-dialog-centered(role="document")
      .modal-content
        button(type="button" class="close" data-dismiss="modal" aria-label="Close")
          span(aria-hidden="true") &times;
        .modal-body
          .form-1__wrapper
            form.main-form(action="")
              h3.form-title.text-uppercase#modalFormLabel оставь заявку и мы перезвоним в течение 3-х минут
              .form-1__container
                .input-container
                  input.form-control(type="text", name="name", placeholder="Имя*")
                .input-container
                  input.form-control(type="tel", name="tel", placeholder="Телефон*")
                .input-container
                  textarea.form-control(name="question", cols="auto", rows="10", placeholder="Вопрос*")
                .submit-container.text-center
                  button.btn.btn-outline-light.btn-form-1(type="submit") Отправить

  section.main-offer
